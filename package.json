{
  "name": "generator-sessionm",
  "version": "1.0.0",
  "description": "Initializes a SessionM application.",
  "scripts": {
    "clean": "rm -rf ./dist && mkdir dist",
    "build": "npm run clean && node_modules/babel-cli/bin/babel.js lib/generators --out-dir ./dist --copy-files",
    "test": "NODE_ENV=test ./node_modules/nyc/bin/nyc.js ./node_modules/mocha/bin/mocha --opts .mocharc --colors",
    "test:tdd": "npm test -- --watch"
  },
  "files": [
    "generators"
  ],
  "babel": {
    "env": {
      "test": {
        "presets": [
          "es2015"
        ],
        "plugins": [
          "transform-decorators-legacy",
          "transform-object-rest-spread",
          "syntax-class-properties",
          "transform-class-properties",
          "rewire"
        ]
      }
    }
  },
  "nyc": {
    "extension": [
      ".js"
    ],
    "exclude": [
      "**/templates/*",
      "dist",
      "test"
    ],
    "all": true
  },
  "repository": {
    "type": "git",
    "url": "ssh://git@stash.o.sessionm.com:7999/es/docker-compose-base.git"
  },
  "keywords": [
    "yeoman",
    "app",
    "docker"
  ],
  "author": "Clinton Christian",
  "license": "MIT",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^4.0.2",
    "faker": "^4.1.0",
    "mocha": "^3.4.2",
    "yo": "^2.0.0",
    "next": "^2.4.9",
    "nyc": "^11.0.2",
    "sinon": "^2.3.5",
    "sinon-chai": "^2.11.0",
    "yeoman-test": "^1.6.0"
  },
  "dependencies": {
    "yeoman-generator": "^1.1.1"
  }
}
